{
  "collectionName": "components_custom_object_urls",
  "info": {
    "displayName": "ObjectURL",
    "icon": "cube",
    "description": ""
  },
  "options": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "minLength": 2,
      "maxLength": 70
    },
    "description": {
      "type": "string",
      "required": true,
      "minLength": 2,
      "maxLength": 100
    },
    "url": {
      "type": "string",
      "required": true,
      "regex": "^https?:\\/\\/(?:www\\.)?[-a-zA-Z0-9@:%._\\+~#=]{1,256}\\.[a-zA-Z0-9()]{1,6}\\b(?:[-a-zA-Z0-9()@:%_\\+.~#?&\\/=]*)$"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    }
  }
}
