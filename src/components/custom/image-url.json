{
  "collectionName": "components_custom_image_urls",
  "info": {
    "displayName": "ImgURL",
    "icon": "picture",
    "description": ""
  },
  "options": {},
  "attributes": {
    "img": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "title": {
      "type": "string",
      "required": true,
      "minLength": 2,
      "maxLength": 70
    },
    "url": {
      "type": "text",
      "required": true,
      "regex": "^https?:\\/\\/(?:www\\.)?[-a-zA-Z0-9@:%.,_\\+~#=]{1,256}(?:.[a-zA-Z0-9()]{1,6})\\b(?:[-a-zA-Z0-9()@:%_\\+.,~#?&\\/=]*)$"
    }
  }
}
